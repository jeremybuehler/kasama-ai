{
  "name": "kasama",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@reduxjs/toolkit": "^2.6.1",
    "@supabase/supabase-js": "^2.57.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-query": "^5.84.1",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.1",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.484.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.55.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.0.2",
    "react-router-hash-link": "^2.4.3",
    "recharts": "^2.15.2",
    "redux": "^5.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-elevation": "^2.0.0",
    "tailwindcss-fluid-type": "^2.0.7",
    "zod": "^4.0.15",
    "zustand": "^5.0.7"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "cross-env NODE_ENV=production node scripts/validate-env.js && vite build",
    "build:skip-validation": "vite build",
    "preview": "vite preview --host",
    "serve": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:performance": "npm run test:e2e -- --grep=\"@performance\"",
    "test:all": "npm run test:run && npm run test:e2e",
    "prepare": "node -e \"if(process.env.NODE_ENV !== 'production' && !process.env.SKIP_HUSKY) { try { require('child_process').execSync('husky install', {stdio: 'inherit'}); } catch(e) { console.log('Husky install skipped:', e.message); } }\"",
    "husky:install": "node -e \"if(process.env.NODE_ENV !== 'production' && !process.env.SKIP_HUSKY) { try { require('child_process').execSync('husky install', {stdio: 'inherit'}); } catch(e) { console.log('Husky install skipped:', e.message); } }\"",
    "postinstall": "node -e \"if(process.env.NODE_ENV !== 'production' && !process.env.SKIP_HUSKY) { try { require('child_process').execSync('npm run husky:install', {stdio: 'inherit'}); } catch(e) { console.log('Husky install skipped:', e.message); } }\"",
    "launch:validate": "node scripts/deployment-validator.js",
    "launch:health": "node scripts/production-health-check.js",
    "launch:monitor-setup": "node scripts/monitoring-setup.js",
    "launch:rollback": "node scripts/rollback-strategy.js",
    "launch:full-validation": "npm run launch:validate && npm run typecheck && npm run lint && npm run build",
    "launch:production": "node scripts/production-launch.js",
    "launch:production-safe": "node scripts/production-launch.js --auto-rollback"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/line-clamp": "^0.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query-devtools": "^5.85.3",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "10.4.2",
    "axe-core": "^4.10.2",
    "axe-playwright": "^2.0.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^8.0.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^16.1.4",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "3.4.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vite-tsconfig-paths": "3.6.0",
    "vitest": "^3.2.4"
  }
}
